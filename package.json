    "owl.carousel": "^2.3.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "http-server": "^0.12.1",
    "laravel-mix": "^4.0.0",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.26.3",
    "sass-loader": "^7.3.1"
  }
